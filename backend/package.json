{
  "name": "yenengalabs-backend",
  "version": "1.0.0",
  "description": "Backend API for YenengaLabs",
  "main": "server.ts",
  "scripts": {
    "start": "bun server.ts",
    "dev": "bun --watch server.ts",
    "seed": "bun data/migrate.ts",
    "test": "bun test",
    "backfill:upload-ownership": "bun scripts/backfillUploadOwnership.ts",
    "db:generate": "bunx drizzle-kit generate --config=data/drizzle.config.ts",
    "db:migrate": "bunx drizzle-kit migrate --config=data/drizzle.config.ts",
    "db:push": "bunx drizzle-kit push --config=data/drizzle.config.ts",
    "db:studio": "bunx drizzle-kit studio --config=data/drizzle.config.ts"
  },
  "dependencies": {
    "@fal-ai/client": "^1.6.4",
    "drizzle-orm": "^0.45.1",
    "openai": "^5.20.1",
    "postgres": "^3.4.8"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.3.15",
    "drizzle-kit": "^0.31.9"
  }
}
